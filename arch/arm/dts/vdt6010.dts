&ldb {
	status = "okay";
	
	lvds-channel@0 {
		crtc = "ipu1-di0";
		status = "disabled";
	};

	lvds-channel@1 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <24>;
		primary;
		crtc = "ipu1-di0";
		status = "okay";
		
		display-timings {
			native-mode = <&timing0>;
			timing0: AM-1280800NTZQW-T10H {
					/*
					* Tv = hactive + hback-porch + hfront-porch + hsync-len
					* Th = vactive + vback-porch + vfront-porch + vsync-len
					*/
					clock-frequency = <71100000>;
					hactive = <1280>;
					vactive = <800>;
					hback-porch = <50>;
					hfront-porch = <50>;
					vback-porch = <10>;
					vfront-porch = <10>;
					hsync-len = <60>;
					vsync-len = <3>;
			};
		};
	};
};

